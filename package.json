{
  "name": "typescript-doctest",
  "version": "1.1.0",
  "description": "Doc Test Generator for TypeScript Projects",
  "main": "lib/src/index.js",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "test": "jest --config jestconfig.json",
    "doc": "typedoc --out ./doc",
    "build": "tsc"
  },
  "files": [
    "lib/src/**/*",
    "bin/ts-doctest"
  ],
  "homepage": "https://gitlab.com/BonsaiDen/ts-doctest#readme",
  "bugs": {
    "url": "https://gitlab.com/BonsaiDen/ts-doctest/issues"
  },
  "bin": {
    "ts-doctest": "./bin/ts-doctest"
  },
  "keywords": [
    "tests",
    "typescript",
    "testing",
    "documentation",
    "doc",
    "typedocplugin",
    "typedoc"
  ],
  "author": "Ivo Wetzel",
  "license": "MIT",
  "dependencies": {
    "markdown-it": "^10.0.0",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/markdown-it": "0.0.9",
    "@types/node": "^13.7.7",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1"
  }
}
